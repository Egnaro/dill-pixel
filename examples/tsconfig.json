{
  "extends": "../config/tsconfig.base.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "dill-pixel/*": [
        "../src/*"
      ]
    },
    "composite": true
  },
  "references": [
    {
      "path": ".."
    }
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.json"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
